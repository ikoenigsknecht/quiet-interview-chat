{
  "name": "quiet-interview-chat",
  "version": "0.0.1",
  "description": "Websocket API backend for a chat application",
  "main": "src/index.ts",
  "author": "ikoenigsknecht <ikoenigsknecht@users.noreply.github.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "docker:up": "docker compose up -d",
    "init:env": "yarn docker:up && sh ./cassandra.local.init.sh",
    "start": "yarn docker:up && node ./bin/index.js",
    "start:dev": "yarn docker:up && ts-node ./src/index.ts",
    "watch": "yarn docker:up && tsc && tsc-watch --noClear -p ./tsconfig.json --onSuccess \"node ./bin/index.js\"",
    "test": "jest",
    "script:test-paired-clients": "ts-node ./e2e/test_with_paired_clients.ts",
    "script:test-client-validity": "ts-node ./e2e/test_client_validity"
  },
  "dependencies": {
    "cassandra-driver": "^4.7.2",
    "dotenv": "^16.4.4",
    "ioredis": "^5.3.2",
    "kafkajs": "^2.2.4",
    "koa": "^2.15.0",
    "koa-bodyparser": "^4.4.1",
    "koa-logger": "^3.2.1",
    "koa-router": "^12.0.1",
    "lodash": "^4.17.21",
    "log4js": "^6.9.1",
    "luxon": "^3.4.4",
    "node": "^21.6.2",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/cassandra-driver": "^4.2.0",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.12",
    "@types/kafkajs": "^1.9.0",
    "@types/koa": "^2.14.0",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa-logger": "^3.1.5",
    "@types/koa-router": "^7.4.8",
    "@types/lodash": "^4.14.202",
    "@types/log4js": "^2.3.5",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.11.19",
    "@types/socket.io-client": "^3.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2"
  }
}
